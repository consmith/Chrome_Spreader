{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension creates a speed reading mode for any article on Chrome",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
		//"default_popup":"popup.html"
  },
	"background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "storage",
		"https://*/*",
    "http://*/*",
    "tabs"
  ]
}
